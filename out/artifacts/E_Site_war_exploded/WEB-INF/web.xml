<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>uploadServlet</servlet-name>
        <servlet-class>servlet.eBookUploadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>uploadServlet</servlet-name>
        <url-pattern>/FileUploadServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>musicUplaod</servlet-name>
        <servlet-class>servlet.MusicUploadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>musicUplaod</servlet-name>
        <url-pattern>/musicFileUpload</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>movieUpload</servlet-name>
        <servlet-class>servlet.MovieUploadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>movieUpload</servlet-name>
        <url-pattern>/movieFileUpload</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>showUpload</servlet-name>
        <servlet-class>servlet.ShowUploadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>showUpload</servlet-name>
        <url-pattern>/ShowFileUpload</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>postUpload</servlet-name>
        <servlet-class>servlet.postUploadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>postUpload</servlet-name>
        <url-pattern>/postCommentUpload</url-pattern>
    </servlet-mapping>
</web-app>